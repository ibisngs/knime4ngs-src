<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="./scatterplot.png" type="Visualizer">
	<name>Scatterplot</name>
    
    <shortDescription>
        Create Scatterplot from given data
    </shortDescription>
    
    <fullDescription>
        <intro>Create Scatterplot from given data</intro>
        <option name="Select Variables">Choose which variables shall be normalized</option>
    </fullDescription>
    
    <ports>
        <inPort index="0" name="data table">Data with variables in the columns and samples in the rows</inPort>
        <outPort index="0" name="Image">The produced plot</outPort>
            </ports>    
    <views>
        <view index="0" name="R Logs">STDOUT and STDERR of the according R process</view>
    </views>
</knimeNode>
